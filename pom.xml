<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>fr.weefle.waze</groupId>
  <artifactId>SkWaze</artifactId>
  <version>2.1.3</version>
  <repositories>
  <repository>
          <id>elmakers-repo</id>
          <url>http://maven.elmakers.com/repository/</url>
        </repository>
        <repository>
    <id>Weefle-weefle</id>
    <url>https://packagecloud.io/Weefle/weefle/maven2</url>
    <releases>
      <enabled>true</enabled>
    </releases>
    <snapshots>
      <enabled>true</enabled>
    </snapshots>
  </repository>
	<repository>
		<id>inventive-repo</id>
		<url>https://repo.inventivetalent.org/content/groups/public/</url>
	</repository>
        </repositories>
        <dependencies>
        <dependency>
            <groupId>org.spigotmc</groupId>
            <artifactId>spigot</artifactId>
            <version>1.12.2-R0.1-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
		<groupId>fr.rhaz</groupId>
		<artifactId>socket4mc</artifactId>
		<version>3.1</version>
	</dependency>
        <dependency>
		<groupId>org.inventivetalent</groupId>
		<artifactId>bossbarapi</artifactId>
		<version>2.4.1</version>
	</dependency>
	<dependency>
    <groupId>org.mcstats.bukkit</groupId>
    <artifactId>metrics-lite</artifactId>
    <version>R8-SNAPSHOT</version>
</dependency>
<dependency>
  <groupId>fr.rhaz</groupId>
  <artifactId>socket4mc</artifactId>
  <version>3.2.8</version>
</dependency>
        <dependency>
            <groupId>ch.njol</groupId>
            <artifactId>skript</artifactId>
            <version>2.2-SNAPSHOT</version>
        </dependency>
        <dependency>
    <groupId>com.google.code.findbugs</groupId>
    <artifactId>jsr305</artifactId>
    <version>3.0.2</version>
</dependency>
        
</dependencies>
  <build>
    <sourceDirectory>SkWaze/src</sourceDirectory>
    <resources>
      <resource>
        <directory>SkWaze/src</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.7.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>